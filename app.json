{
  "expo": {
    "name": "Prayer Accountability App",
    "slug": "PrayerAccountabilityApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "scheme": "PrayerAccountabilityApp",
    "plugins": [
      "expo-router",
      "expo-location",
      [
        "expo-notifications",
        {
          "defaultChannel": "default",
          "sounds": [
            "./assets/sounds/adhan1.wav"
          ],
          "ios": {
            "sounds": [
              "./assets/sounds/adhan1.wav"
            ]
          },
          "android": {
            "sounds": [
              "./assets/sounds/adhan1.wav"
            ]
          },
          "enableBackgroundRemoteNotifications": false
        }
      ],
      "expo-audio"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.anonymous.PrayerAccountabilityApp"
    },
    "android": {
      "permissions": [
        "POST_NOTIFICATIONS",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS"
      ],
      "package": "com.anonymous.PrayerAccountabilityApp"
    },
    "extra": {
      "eas": {
        "projectId": "39aa4603-e266-424a-965e-fbd897f2aa24"
      },
      "router": {}
    },
    "owner": "kingquinn"
  }
}
