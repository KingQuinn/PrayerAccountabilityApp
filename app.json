{
  "expo": {
    "name": "PrayerAccountabilityApp",
    "slug": "PrayerAccountabilityApp",
    "scheme": "PrayerAccountabilityApp",
    "plugins": [
      "expo-router",
      "expo-location",
      "expo-notifications"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.anonymous.PrayerAccountabilityApp"
    },
    "android": {
      "permissions": [
        "POST_NOTIFICATIONS",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ]
    }
  }
}
