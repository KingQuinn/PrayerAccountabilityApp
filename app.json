{
  "expo": {
    "name": "PrayerAccountabilityApp",
    "slug": "PrayerAccountabilityApp",
    "scheme": "PrayerAccountabilityApp",
    "plugins": [
      "expo-router",
      "expo-location",
      [
        "expo-notifications",
        {
          "defaultChannel": "default",
          "sounds": [
            "./assets/sounds/adhan1.wav"
          ],
          "ios": {
            "sounds": [
              "./assets/sounds/adhan1.wav"
            ]
          },
          "android": {
            "sounds": [
              "./assets/sounds/adhan1.wav"
            ]
          },
          "enableBackgroundRemoteNotifications": false
        }
      ],
      "expo-audio"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.anonymous.PrayerAccountabilityApp"
    },
    "android": {
      "permissions": [
        "POST_NOTIFICATIONS",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "POST_NOTIFICATIONS",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ],
      "package": "com.anonymous.PrayerAccountabilityApp"
    },
    "extra": {
      "eas": {
      }
    }
  }
}
